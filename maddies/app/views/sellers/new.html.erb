<%= form_for @seller do |f| %>
  <%= f.label :name %><br>
  <%= f.text_field :name %><br><br>
  <%= f.label :user_name %><br>
  <%= f.text_field :user_name %><br><br>
  <%= f.label :address %><br>
  <%= f.text_field :address %><br><br>
  <%= f.label :email %><br>
  <%= f.text_field :email %><br><br>
  <%= f.label :img_url %><br>
  <%= f.text_field :img_url %><br><br>
  <%= f.label :bank_account %><br>
  <%= f.text_field :bank_account %><br><br>
  <%= f.label :password %><br>
  <%= f.password_field :password %><br><br>
  <%= f.label :password_confirmation %><br>
  <%= f.text_field :password_confirmation %><br><br>
  <%= f.submit %>
<% end %>
